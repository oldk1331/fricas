)expose UnittestCount UnittestAux Unittest

testsuite "seg.spad"

testcase "plus and minus for Segment"

testEquals("1 + (3..6)", "4..7")
testEquals("(-10 .. -3) + 10", "0..7")
testEquals("(0..6) - 1", "-1..5")

testcase "plus and minus for UniversalSegment"

testEquals("4 + (-6..)", "-2..")
testEquals("(1..) + -3", "-2..")
testEquals("(3..) - 5", "-2..")

statistics()
